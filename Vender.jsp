<body>
	  
	<section id="form"><!--form-->
		<div class="container">
			<h2>Vender Registration Than Login</h2><br>
			<div class="row">
				<div class="col-sm-4 col-sm-offset-1">
					<div class="login-form"><!--login form-->
						<h2>Login to your account</h2>
						<form action="VenderReg.jsp">
							<input type="text" name="name" placeholder="Name" required="required" />
							<input type="email" name="email"  placeholder="Email Address" required="required"/>
							<br>
							<button type="submit" class="btn btn-default">Login</button>
							
						</form>
						
					</div><!--/login form-->
				</div>
				
				<div class="col-sm-1">
					<h2 class="or">OR</h2>
				</div>
				<div class="col-sm-4">
					<div class="signup-form"><!--sign up form-->
						<h2>New User Signup!</h2>
						<form action="VenderReg.jsp">
							<input type="text" name="name" placeholder="Name" required="This field is required"/>
							<input type="email" name="email" placeholder="Email Address" required="This field is required"/>
							<input type="password" name="password" placeholder="Password" required="This field is required"/>
							<button type="submit" class="btn btn-default">Signup</button>
						</form>
					</div><!--/sign up form-->
				</div>
			</div>
		</div>
	</section><!--/form-->
							
</body>
</html>